
global proc mayaMVGToolValues(string $toolName)
{
    string $parent = `toolPropertyWindow -q -location`;
    setParent ($parent);
    string $icon = "mayaMVGTool.png";
    string $help = "";
    toolPropertySetCommon $toolName $icon $help;
    toolPropertySelect mvgToolLayout;
    mayaMVGToolOptionValues($toolName);
}

